{
  "name": "@ttg-club/dice-roller-parser",
  "type": "module",
  "version": "0.2.6",
  "packageManager": "pnpm@10.28.1",
  "description": "A javascript dice roller that parses roll20 format strings and generates rolled outputs",
  "author": "Ben Morton <ben.morton91@gmail.com>",
  "contributors": [
    {
      "name": "Frank Ali",
      "email": "frankieali4@gmail.com"
    },
    {
      "name": "TTG Club",
      "url": "https://github.com/TTG-Club"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/ttg-club/dice-roller-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttg-club/dice-roller-parser.git"
  },
  "bugs": {
    "url": "https://github.com/ttg-club/dice-roller-parser/issues"
  },
  "keywords": [
    "dice",
    "roller",
    "roll",
    "roll20",
    "rpg",
    "parser",
    "tabletop",
    "dnd",
    "d&d",
    "дайсы",
    "кубы",
    "кости",
    "кубики",
    "бросок",
    "нри",
    "днд"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr gen && tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "gen": "peggy --format es --output src/diceroll.js src/diceroll.peggy",
    "test": "vitest",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "sync-ai-rules": "node .scripts/sync-ai-rules.js",
    "prepare": "simple-git-hooks"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@antfu/ni": "^28.2.0",
    "@svifty7/eslint-config": "^0.0.11",
    "@types/node": "^22.10.0",
    "bumpp": "^10.4.0",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.0",
    "peggy": "^4.2.0",
    "rimraf": "^6.0.0",
    "simple-git-hooks": "^2.11.0",
    "tsdown": "^0.19.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "post-checkout": "pnpm sync-ai-rules",
    "post-merge": "pnpm sync-ai-rules",
    "post-rewrite": "pnpm sync-ai-rules"
  }
}
